@using Volo.Abp.UI.Navigation
@inject IJSRuntime JS
@inherits LayoutComponentBase
<SideNav></SideNav>
<nav class="navbar navbar-expand-md navbar-dark shadow-sm main-navbar" id="main-navbar">
    <button class="navbar-toggler" type="button" @onclick="SideNavToggle" id="navbarToggler">
    <span class="navbar-toggler-icon"></span>
    </button>
    <Branding />
    <NavMenu />        
    <NavToolbar />
</nav>
<div class="container-fluid page-container">
    <PageAlert />
    <div style="min-height:1500px">
    @Body
    </div>
    <UiMessageAlert />
    <UiNotificationAlert />
    <UiPageProgress />
</div>
@code{
    private void SideNavToggle()
    {
        JS.InvokeVoidAsync("sideNavToggle");
    }    
}